{
  "name": "SoundCloud to Spotify",
  "description": "Chrome extension to transfer Soundcloud playlists, albums, and songs directly into your Spotify account",
  "manifest_version": 2,
  "version": "1.0.0",
  "icons": {
    "16": "logo-64.png",
    "48": "logo-64.png",
    "128": "logo-64.png"
  },
  "browser_action": {
    "default_icon": {
      "16": "logo-64.png",
      "48": "logo-64.png"
    },
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": [
        "*://soundcloud.com/**"
      ],
      "js": [
        "content.js"
      ] 
    }
  ],
  "permissions": [
    "https://*.spotify.com/"
  ]
}